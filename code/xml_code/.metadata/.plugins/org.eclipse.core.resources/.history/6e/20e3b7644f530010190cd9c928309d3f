package com.demo;

import java.util.Scanner;

public class HelloWorld {
	
	public static void main(String[] args) {
		
		System.out.println("PE a no");
		Scanner scanner=new Scanner(System.in);
		int val=scanner.nextInt();
		
		for(int i=0;i<val;i++) {
			for(int j=0;j<=i;j++) {
				System.out.print("*");
			}
			System.out.println();
		}
	}

}
