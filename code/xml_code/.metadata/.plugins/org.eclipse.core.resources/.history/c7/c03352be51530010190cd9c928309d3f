package com.demo;
class Employee{
	//C++ vs java (default)
	private int id;
	private String name;
	
	public Employee(int id, String name) {
		this.id = id;
		this.name = name;
	}
	
	public void print() {
		System.out.println(" id : "+ id +" name: "+ name);
	}
	
}
public class DemoEmployee {
	
	public static void main(String[] args) {
		Employee employee=new Employee(2, "raja");
		employee.print();
	}

}
